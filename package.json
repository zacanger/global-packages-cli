{
  "name": "global-packages-cli",
  "description": "cli for global-packages",
  "version": "2.0.7",
  "author": {
    "email": "zac@zacanger.com",
    "name": "Zac Anger",
    "url": "http://zacanger.com"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/zacanger"
  },
  "license": "LGPL-3.0",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "preferGlobal": true,
  "bin": {
    "global-packages": "./index.js",
    "global-packages-cli": "./index.js"
  },
  "scripts": {
    "build": "tsc",
    "prebuild": "npm t",
    "preversion": "npm run build",
    "test": "run-s -s test:ts test:lint",
    "test:lint": "eslint -c .eslintrc.json src",
    "test:ts": "tsc --noEmit"
  },
  "homepage": "https://github.com/zacanger/global-packages-cli#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/global-packages-cli.git"
  },
  "bugs": "https://github.com/zacanger/global-packages-cli/issues",
  "keywords": [
    "global",
    "modules",
    "node",
    "npm",
    "packages"
  ],
  "dependencies": {
    "global-packages": "1.0.2"
  },
  "devDependencies": {
    "@types/node": "14.11.2",
    "eslint": "7.9.0",
    "eslint-plugin-zacanger": "2.1.3",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "sortpack": "2.1.9",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ],
    "package.json": [
      "sortpack"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
